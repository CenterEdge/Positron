        <file src="..\src\Positron.Server\Positron.Server.targets" target="build\net452\Positron.Server.targets" />
        <file src="..\src\Positron.Server\bin\Release\Positron.Server.dll" target="lib\net452\Positron.Server.dll" />
		<file src="..\src\Positron.Server\bin\Release\Positron.Server.xml" target="lib\net452\Positron.Server.xml" />
